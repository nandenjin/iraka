{
  "name": "@nandenjin/iraka",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts",
    "lintfix": "eslint . --ext ts --fix",
    "format": "prettier --write \"**/*.{ts,json,yml,html,md}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "vue": "3.2.37"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "@vitejs/plugin-vue": "2.3.3",
    "eslint": "8.17.0",
    "eslint-config-prettier": "8.5.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.1",
    "prettier": "2.6.2",
    "typescript": "4.7.3",
    "vite": "2.9.12",
    "vite-plugin-chrome-extension": "0.0.7",
    "vue-tsc": "0.37.3"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix",
    "*.{ts,json,yml,html,md}": "prettier --write"
  }
}
